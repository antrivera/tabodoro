{
  "manifest_version": 2,

  "name": "Tabodoro",
  "description": "Turn new tab page into a pomodoro timer to boost productivity and focus",
  "version": "1.0",

  "browser_action": {
   "default_icon": "timer-icon.png",
   "default_title": "Tabodoro"
 },
 "chrome_url_overrides": {
   "newtab": "timer.html"
 },
 "permissions": [
   "storage",
   "https://ajax.googleapis.com/"
 ]

}
